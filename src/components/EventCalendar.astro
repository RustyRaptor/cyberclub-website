---
import type { Page } from "astro";
import type { CollectionEntry } from "astro:content";
import IconArrowLeft from "@/assets/icons/IconArrowLeft.svg";
import IconArrowRight from "@/assets/icons/IconArrowRight.svg";
import LinkButton from "./LinkButton.astro";

export interface Props {
        page: Page<CollectionEntry<"blog">>;
}

const { page } = Astro.props;
---

{
<style unselectable="on">
#wrap {
width:100%;
height:50rem;
padding:0;
position:relative;
left:0px;
top:0px;
overflow:hidden;
}
#frame {
width:100%;
height:50rem;
position:relative;
left:0px;
top:0px;
}
#frame {
-ms-zoom:0.7;
}
</style>
<div id="wrap" unselectable="on">
    <iframe id="frame" src="https://outlook.office365.com/calendar/published/fd13e8ce2f8846fcb1fec266f25c8dbd@nmsu.edu/ed6799eea93d475eb5c44c1c0ccd3aa91901734758889560687/calendar.html"></iframe>
</div>
}
